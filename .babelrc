{
  "plugins": [
    ["transform-flow-strip-types"]
  ],
  "env": {
    "cjs": {
      "presets": [
        ["latest", {"es2015": {"loose": true}}]
      ],
      "plugins": [
        ["add-module-exports"]
      ]
    },
    "es": {
      "presets": [
        ["latest", {"es2015": {"loose": true, "modules": false}}]
      ]
    },
    "test": {
      "presets": [
        ["latest", {"es2015": {"loose": true, "modules": "commonjs"}}]
      ]
    }
  }
}
